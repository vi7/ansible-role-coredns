$ORIGIN 200.168.192.in-addr.arpa.
$TTL 3h

@   IN SOA  molecule.home. root.home. (
            2021090601  ; serial in the format YYYYMMDD##
            24h         ; refresh after 24 hours
            2h          ; retry after 2 hours
            1w          ; expire after 1 week
            1h )        ; minimum TTL of 1 hour
;
;
    IN NS   molecule.home.
;
;
50    IN PTR  molecule.home.
